{
  "name": "@ctf/ui",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --out-dir dist",
    "dev": "tsup --watch src/index.ts --dts --format cjs,esm --out-dir dist",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "react-markdown": "^9.0.1",
    "rehype-prism-plus": "^2.0.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^2.5.2",
    "@tanstack/react-table": "^8.20.5"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.4",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.9.0",
    "postcss": "^8.4.41",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "^3.4.10",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  }
}
